<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
    <!--登录-->
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <!--注册-->
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <!--用户界面-->
    <servlet>
        <servlet-name>UserInterFace</servlet-name>
        <servlet-class>UserInterFace</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserInterFace</servlet-name>
        <url-pattern>/userInterface</url-pattern>
    </servlet-mapping>
    <!--管理员界面-->
    <servlet>
        <servlet-name>AdminInterFace</servlet-name>
        <servlet-class>AdminInterFace</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminInterFace</servlet-name>
        <url-pattern>/adminInterface</url-pattern>
    </servlet-mapping>
</web-app>